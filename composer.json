{
  "name": "frankfoerster/cakephp-asset",
  "description": "Provides a CakePHP 3.x AssetHelper to selectively add last modified timestamps to css and js assets.",
  "type": "cakephp-plugin",
  "keywords": ["cakephp", "helper", "assets", "timestamp"],
  "homepage": "https://github.com/frankfoerster/cakephp-asset",
  "license": "MIT",
  "authors": [
    {
      "name": "Frank FÃ¶rster",
      "homepage": "https://frankfoerster.com",
      "role": "Developer"
    }
  ],
  "scripts": {
    "test": "phpunit"
  },
  "require": {
    "php": ">=8.3.0"
  },
  "require-dev": {
    "cakephp/cakephp": "^5.0.5",
    "phpunit/phpunit": "11.0.2",
    "cakephp/cakephp-codesniffer": "5.1.1"
  },
  "autoload": {
    "psr-4": {
      "FrankFoerster\\Asset\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TestApp\\": "tests/testapp/src",
      "FrankFoerster\\Asset\\Test\\": "tests",
      "FooBar\\TestPlugin\\": "tests/testapp/plugins/FooBar/TestPlugin/src",
      "Namespaced\\Plugin\\": "tests/testapp/plugins/Namespaced/Plugin/src",
      "Namespaced2\\TestPlugin\\": "tests/testapp/plugins/Namespaced2/TestPlugin/src",
      "TestPlugin\\": "tests/testapp/plugins/TestPlugin/src"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
